{
  "QWidget": {
    "background-color": "rgba(180,190,210,0.95)",
    "color": "#22223b",
    "font-family": "Montserrat, Arial, sans-serif",
    "font-size": "15px"
  },
  "QDialog": {
    "background-color": "rgba(180,190,210,0.95)",
    "color": "#22223b",
    "font-family": "Montserrat, Arial, sans-serif",
    "font-size": "15px"
  },
  "QLabel": {
    "color": "#22223b",
    "font-size": "15px",
    "background": "transparent"
  },
  "QGroupBox": {
    "border": "1px solid none",
    "border-radius": "18px",
    "margin-top": "10px",
    "color": "#22223b",
    "font-size": "15px"
  },
  "QGroupBox::title": {
    "subcontrol-origin": "margin",
    "left": "10px",
    "padding": "0 3px 0 3px"
  },
  "QPushButton": {
    "background-color": "{button_bg}",
    "color": "{button_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "6px 18px"
  },
  "QPushButton:hover": {
    "background-color": "{button_hover}"
  },
  "QPushButton:pressed": {
    "background-color": "{button_active}"
  },
  "QPushButton:disabled": {
    "background-color": "#cccccc",
    "color": "#888888"
  },
  "QPushButton#FloatingActionButton": {
    "background-color": "{button_bg}",
    "color": "{button_fg}",
    "border-radius": "50%",
    "border": "none",
    "font-size": "15px",
    "width": "56px",
    "height": "56px"
  },
  "QPushButton#FloatingActionButton:hover": {
    "background-color": "{button_hover}"
  },
  "QPushButton#FloatingActionButton:pressed": {
    "background-color": "{button_active}"
  },
  "QLineEdit": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QTextEdit": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QPlainTextEdit": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QSpinBox": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QDoubleSpinBox": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QDateEdit": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QTimeEdit": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QComboBox": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px",
    "padding": "4px 8px",
    "min-width": "420px"
  },
  "QComboBox QAbstractItemView": {
    "background-color": "{input_bg}",
    "color": "{input_fg}",
    "selection-background-color": "#a3bffa"
  },
  "QLabel.chip": {
    "background": "{chip_bg}",
    "color": "{chip_fg}",
    "border-radius": "16px",
    "padding": "4px 12px",
    "font-size": "15px"
  },
  "QTabWidget::pane": {
    "border": "1px solid none",
    "border-radius": "18px"
  },
  "QTabBar::tab": {
    "background": "{button_bg}",
    "color": "{button_fg}",
    "border": "1px solid none",
    "border-radius": "18px",
    "padding": "6px 18px",
    "font-size": "15px"
  },
  "QTabBar::tab:selected": {
    "background": "{button_active}",
    "color": "#a3bffa"
  },
  "QTabBar::tab:hover": {
    "background": "{button_hover}"
  },
  "QListView": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border": "1px solid none",
    "border-radius": "18px",
    "font-size": "15px"
  },
  "QTreeView": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border": "1px solid none",
    "border-radius": "18px",
    "font-size": "15px"
  },
  "QListView::item:selected": {
    "background": "#a3bffa",
    "color": "#22223b"
  },
  "QTreeView::item:selected": {
    "background": "#a3bffa",
    "color": "#22223b"
  },
  "QTableView": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border": "1px solid none",
    "border-radius": "18px",
    "font-size": "15px",
    "gridline-color": "none"
  },
  "QHeaderView::section": {
    "background": "{button_bg}",
    "color": "{button_fg}",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QProgressBar": {
    "border": "1px solid none",
    "border-radius": "18px",
    "text-align": "center",
    "background": "{input_bg}",
    "color": "{input_fg}",
    "font-size": "15px"
  },
  "QProgressBar::chunk": {
    "background-color": "#a3bffa",
    "width": "20px"
  },
  "QSlider::groove:horizontal": {
    "border": "1px solid none",
    "height": "8px",
    "background": "{input_bg}",
    "border-radius": "4px"
  },
  "QSlider::handle:horizontal": {
    "background": "#a3bffa",
    "border": "1px solid none",
    "width": "18px",
    "margin": "-5px 0",
    "border-radius": "9px"
  },
  "QSplitter::handle": {
    "background": "none"
  },
  "QWidget#NavigationPanel": {
    "background": "rgba(180,190,210,0.95)",
    "color": "#22223b",
    "border-radius": "18px"
  },
  "QWidget#HelpPanel": {
    "background": "rgba(180,190,210,0.95)",
    "color": "#22223b",
    "border-radius": "18px"
  },
  "QWidget#CenterPanel": {
    "background": "rgba(180,190,210,0.95)",
    "color": "#22223b",
    "border-radius": "18px"
  },
  "QToolBar": {
    "background": "{button_bg}",
    "border-bottom": "1px solid none",
    "spacing": "6px"
  },
  "QFormLayout QLabel": {
    "color": "#22223b",
    "font-size": "15px"
  },
  "QFormLayout QLineEdit": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QFormLayout QDateEdit": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QFormLayout QSpinBox": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QFormLayout QTextEdit": {
    "background": "{input_bg}",
    "color": "{input_fg}",
    "border-radius": "18px",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QToolTip": {
    "background": "{button_bg}",
    "color": "{button_fg}",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QMenu": {
    "background": "{button_bg}",
    "color": "{button_fg}",
    "border": "1px solid none",
    "font-size": "15px"
  },
  "QMenu::item:selected": {
    "background": "#a3bffa",
    "color": "#22223b"
  },
  "QLabel#FormHeaderLabel": {
    "font-size": "22px",
    "font-weight": "bold",
    "color": "#22223b",
    "margin-bottom": "12px"
  },
  "QLabel#error": {
    "color": "#ff6b6b",
    "border": "1px solid #ff6b6b",
    "background": "#fff0f0"
  },
  "QLineEdit[error=\"true\"]": {
    "color": "#ff6b6b",
    "border": "1px solid #ff6b6b",
    "background": "#fff0f0"
  },
  "QTextEdit[error=\"true\"]": {
    "color": "#ff6b6b",
    "border": "1px solid #ff6b6b",
    "background": "#fff0f0"
  },
  "QLabel.link": {
    "color": "{link_fg}",
    "text-decoration": "underline"
  },
  "QLabel.link:hover": {
    "color": "{link_hover}"
  }
}